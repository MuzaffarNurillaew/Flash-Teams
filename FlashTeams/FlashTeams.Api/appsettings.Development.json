{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "ConnectionStrings": {
        "DefaultConnection": "{CONNECTION_STRING}"
    },
    "JwtSettings": {
        "SecretKey": "{SECRET}{SECRET}{SECRET}{SECRET}{SECRET}{SECRET}{SECRET}{SECRET}{SECRET}",
        "Issuer": "http://localhost:5000",
        "Audience": "http://localhost:5000",
        "ExpirationMinutes": 60
    },
    "Authentication": {
        "Google": {
            "ClientId": "{CLIENT_ID}",
            "ClientSecret": "{CLIENT_SECRET}"
        },
        "Schemes": {
            "Bearer": {
                "ValidAudiences": [
                    "http://localhost:4949",
                    "https://localhost:44362",
                    "http://localhost:5080",
                    "https://localhost:7098"
                ],
                "ValidIssuer": "dotnet-user-jwts"
            }
        }
    },
    "KeyVaultName": "flash-teams-vault"
}